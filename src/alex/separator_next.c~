#include "shell.h"

void    count_and(char *commande, t_second *ini)
{
  int   z;

  z = 0;
  ini->check.nb_and = 0;
  while (commande[z] != '\0')
    {
      if (commande[z] == '&' && commande[z + 1] == '&')
        ini->check.nb_and = 1;
      z++;
    }
}
